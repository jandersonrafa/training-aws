  "main": "index.js",
  "engines": {
    "node": "11.1.0",
    "npm": "6.10.3"
  },
  "scripts": {
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "heroku-prebuild": "npm install --only=production",
    "heroku-postbuild": "webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js && node_modules/.bin/sequelize db:migrate",
    "start": "node ./dist/server.js",
    "debug": "node --inspect-brk=9229 ./dist/server.js",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate-undo": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
